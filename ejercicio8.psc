Algoritmo ejercicio8
	Escribir 'Ponga el sueldo'
	Leer sueldo
	Escribir 'Ponga las ventas'
	Leer v1
	Leer v2
	Leer v3
	comision <- (v1+v2+v3)*0.10
	mensualidad <- (sueldo+comision)
	Escribir 'Su comision'
	Escribir comision
	Escribir 'Su total este mes'
	Escribir mensualidad
FinAlgoritmo
